<%- include("partials/header"); -%>

<body>

    <nav class="navbar navbar-default">
        <div class="container">
            <a class="navbar-brand" href="#">Home</a>
        </div>
    </nav>

    <ol>
        <li>
            <ul class="list-group list-group-flush">
                <h4>Comparision across all districts</h4>
                <li class="list-group-item"><a href="/alcoholAllDist"> Alcohol cases all districts</a></li>
                <li class="list-group-item"><a href="/suicideAllDist"> Suicide cases all districts</a></li>
            </ul>
        </li>

        <li>
            <form action="/perDist" method="POST">
                <div class="form-group">
                    <label for="District Id">District Id</label>
                    <select class="form-control" name="districtId">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="Select Parameter">Parameter</label>
                    <select class="form-control" name="parameter">
                        <option value="alcoholCases">Alcohol Cases</option>
                        <option value="suicideCases">Suicide Cases</option>
                    </select>
                </div>
                <button class="btn btn-primary" type="submit">Submit</button>
            </form>
        </li>
    </ol>

    <!-- External JS libraries -->
    <script src="js/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/d3.min.js"></script>

</body>

<%- include("partials/footer"); -%>